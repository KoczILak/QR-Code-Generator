<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="window" named="True">
    <property name="background">#525f98</property>
    <property name="geometry">320x200</property>
    <property name="height">200</property>
    <property name="resizable">none</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Label" id="label1">
        <property name="anchor">center</property>
        <property name="background">#000000</property>
        <property name="font">{Arial} 24 {bold underline}</property>
        <property name="foreground">#ffffff</property>
        <property name="padding">0</property>
        <property name="text" translatable="yes">QR Code Generator</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relheight">0.25</property>
          <property name="relwidth">1.0</property>
          <property name="relx">0.0</property>
          <property name="rely">0.0</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label2">
        <property name="background">#525f98</property>
        <property name="font">{Arial} 12 {bold}</property>
        <property name="foreground">#ffffff</property>
        <property name="text" translatable="yes">Your Link/numbers(or anything else)</property>
        <layout manager="place">
          <property name="anchor">center</property>
          <property name="relx">0.5</property>
          <property name="rely">0.31</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="linkbox" named="True">
        <layout manager="place">
          <property name="anchor">center</property>
          <property name="relwidth">0.92</property>
          <property name="relx">0.5</property>
          <property name="rely">0.42</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label3">
        <property name="background">#525f98</property>
        <property name="font">{Arial} 12 {bold}</property>
        <property name="foreground">#ffffff</property>
        <property name="text" translatable="yes">Name of your QR Code file</property>
        <layout manager="place">
          <property name="anchor">center</property>
          <property name="relx">0.5</property>
          <property name="rely">0.55</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="namebox" named="True">
        <layout manager="place">
          <property name="anchor">center</property>
          <property name="relwidth">0.92</property>
          <property name="relx">0.5</property>
          <property name="rely">0.66</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="generatebutton" named="True">
        <property name="text" translatable="yes">GENERATE</property>
        <layout manager="place">
          <property name="anchor">center</property>
          <property name="relwidth">0.4</property>
          <property name="relx">0.5</property>
          <property name="rely">0.83</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
